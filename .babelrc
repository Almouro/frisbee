{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "8.9.4",
        "browsers": [ "> 1%", "last 2 versions", "ie >= 9" ]
      }
    }]
  ],
  "plugins": [
    [ "@babel/transform-runtime", {
      "helpers": false,
      "regenerator": true
    }],
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "sourceMaps": false
}
